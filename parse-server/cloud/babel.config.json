{
    "presets": 
      [
        [
          "@babel/preset-typescript",
          {
            "onlyRemoveTypeImports": true,
            "moduleResolution": "node",
            "importsNotUsedAsValues":"remove"
          }
        ]

        
      ],
      "plugins": [
        [
          "babel-plugin-transform-remove-imports", {
            "test": "parse"
          }
        ]
      ]
    
  }